[@REPO_SECTION_NAME@]
name=@REPO_NAME@
baseurl=@REPO_BASE_URL@
failovermethod=priority
enabled=@REPO_ENABLED@
priority=98
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Globus
@REPO_TYPE@

[@REPO_SOURCE_SECTION_NAME@]
name=@REPO_SOURCE_NAME@
baseurl=@REPO_BASE_SOURCE_URL@
failovermethod=priority
enabled=0
priority=98
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Globus
@REPO_TYPE@
